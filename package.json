{
  "name": "chrono-golf-js",
  "version": "1.0.0",
  "description": "A monorepo for Chrono Golf related packages",
  "main": "index.js",
  "scripts": {
    "docs::build": "mkdocs build -c",
    "docker::build": "pnpm --filter tee-snatcher run docker::build",
    "start": "pnpm --filter tee-snatcher run start",
    "start::dry-run": "pnpm --filter tee-snatcher run start --dry-run",
    "stop": "pnpm --filter tee-snatcher run stop",
    "test": "pnpm -r test"
  },
  "keywords": [
    "chrono-golf",
    "lightspeed-golf",
    "golf-api",
    "monorepo"
  ],
  "author": "",
  "license": "ISC",
  "private": true,
  "workspaces": ["chrono-golf-api", "tee-snatcher"]
}
